<% layout("/layouts/boilerplate") %>
<div class="container">
  <div class="row">
    <h2 class="mt-4 mb-3 dash-head">Our Candidates</h2>
    <div class="col-8 offset-3"  style="overflow:visible; display: flex; flex-direction: row; overflow-y: scroll ;">
      <% for(application of applications) {%>
      <ul class="list-group">
        <li class="list-group-item"><%= application.name %></li>
        <li class="list-group-item"><%= application.qualification %></li>
      </ul>
      &nbsp; &nbsp; <%}%>
    </div>
  </div>
</div>
